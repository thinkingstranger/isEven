public boolean isEven(int number){
  if (number == 0){
    return true;
  } else if (number == 1){
    return false;
  } else if (number == 2){
    return true;
  } else if (number == 3){
    return false;
  } else if (number == 4){
    return true;
  } else if (number == 5){
    return false;
  } else if (number == 6){
    return true;
  } else if (number == 7){
    return false;
  } else if (number == 8){
    return true;
  }
